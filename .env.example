# Fina Demo 环境变量配置示例
# 复制此文件为 .env 并填入实际值

# Volcano Engine API 配置（可选，用于 AI 解释和洞察）
VOLCENGINE_API_KEY2=
VOLCENGINE_BASE_URL=https://ark.cn-beijing.volces.com/api/v3
VOLCENGINE_MODEL=kimi-k2-250905

# Volcengine RTC / VoiceChat (Voice Agent)
# Used by `agent/` endpoints: POST /api/rtc/proxyFetch
VOLCENGINE_APP_ID=
VOLCENGINE_APP_KEY=
VOLC_ACCESSKEY=
VOLC_SECRETKEY=
VOLC_REGION=cn-north-1
VOLC_RTC_API_URL=https://rtc.volcengineapi.com

# Optional: CozeBot config (if you use Mode=CozeBot in VoiceChatConfig)
COZEBOT_APIKEY=
COZEBOT_BOT_ID=
COZEBOT_URL=https://api.coze.cn

# Optional: ASR/TTS defaults for VoiceChatConfig (Volcano provider)
# For compatibility with `fuli_survey`, you can set a single Speech AppId and reuse it for both ASR/TTS:
VOLC_SPEECH_APP_ID=
VOLC_SPEECH_ACCESS_TOKEN=
VOLC_SPEECH_SECRET_KEY=

VOLC_ASR_MODE=smallmodel
VOLC_ASR_CLUSTER=volcengine_streaming_common
# For smallmodel you typically only need AppId. For bigmodel you may also need AccessToken/ApiResourceId.
VOLC_ASR_APP_ID=
VOLC_ASR_ACCESS_TOKEN=
VOLC_ASR_API_RESOURCE_ID=
VOLC_TTS_CLUSTER=volcano_icl
VOLC_TTS_APP_ID=
VOLC_TTS_VOICE_TYPE=S_ZHNIkyHJ1
VOLC_TTS_SPEED_RATIO=1.0

# 服务端口配置（可选，默认值已在 docker-compose 中设置）
# PORT=8000
# AGENT_PORT=6203
# WEB_PORT=3201
